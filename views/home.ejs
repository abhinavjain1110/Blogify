<!DOCTYPE html>
<html lang="en">
<head>
    <link href="https://cdn.jsdelivr.net/npm/bootstrap@5.3.3/dist/css/bootstrap.min.css" rel="stylesheet" integrity="sha384-QWTKZyjpPEjISv5WaRU9OFeRpok6YctnYmDr5pNlyT2bRjXh0JMhjY6hW+ALEwIH" crossorigin="anonymous">
    <meta charset="UTF-8">
    <meta name="viewport" content="width=device-width, initial-scale=1.0">    
    <title>Homepage</title>
    <%- include('./partials/style') %>
</head>
<body>
    <%- include('./partials/nav') %>
    <div class="container mt-3 row row-cols-4" ">
        <% blogs.forEach(blog => { %>
            <div class="container">
            <div class="col" style="display:inline-block; flex:initial;">
            <div class="card cs col card border-success">
                <%- include('./partials/style') %>
                <img src="<%= blog.coverImageURL %>" class="card-img-top" >
                <div class="card-body text-bg-dark p-4 ">
                  <h5 class="card-title "><%= blog.title %></h5>
                  <a href="/blog/<%= blog._id %>" class="btn btn-secondary">View</a>
                </div>
              </div>
              </div>
              </div>
        <% }) %>
    </div>
    <%- include('./partials/script') %>
</body>
</html>